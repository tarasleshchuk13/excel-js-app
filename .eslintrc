{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "object-curly-spacing": "off",
        "indent": ["error", 4],
        "arrow-parens": "off",
        "require-jsdoc": "off",
        "eol-last": "off",
        "padded-blocks": "off",
        "no-trailing-spaces": "off",
        "linebreak-style": "off",
        "operator-linebreak": "off"
    },
    "extends": [
        "eslint:recommended",
        "google"
    ]
}