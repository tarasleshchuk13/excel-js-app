{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "object-curly-spacing": "off",
        "indent": ["error", 4],
        "arrow-parens": "off",
        "require-jsdoc": "off",
        "eol-last": "off"
    },
    "extends": [
        "eslint:recommended",
        "google"
    ]
}